{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
            "endOfLine": "auto"
        }
      ]
    },
    "plugins": ["prettier"]
}
